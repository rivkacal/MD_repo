USE_EMPTY_SLOTS=FLAG OPTIONS:YES|NO DEFAULT:YES
USE_COPY_QUEUE=FLAG OPTIONS:YES|NO DEFAULT:NO
CLEAN_SCRATCH_FOLDER=FLAG OPTIONS:YES|NO DEFAULT:NO
BASE_PATH=DIRECTORY NEW WRITABLE DEFAULT:.
